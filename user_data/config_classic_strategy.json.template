{
  "bot_name": "classic-bot",
  "strategy": "classic_strategy.py",
  "dry_run": true,
  "timeframe": "5m",
  "startup_candle_count": 50,
  "max_open_trades": 3,
  "stake_currency": "USDT",
  "stake_amount": 100,
  "tradable_balance_ratio": 0.99,
  "fiat_display_currency": "USD",
  "force_entry_enable": true,
  "exchange": {
    "name": "okx",
    "key": "YOUR_API_KEY_HERE",
    "secret": "YOUR_API_SECRET_HERE",
    "ccxt_config": {
      "enableRateLimit": true,
      "rateLimit": 100
    },
    "ccxt_async_config": {
      "enableRateLimit": true,
      "rateLimit": 100
    },
    "pair_whitelist": [],
    "pair_blacklist": [
      ".*_PREMIUM",
      ".*_DISCOUNT",
      ".*UP/.*",
      ".*DOWN/.*",
      ".*BEAR/.*",
      ".*BULL/.*"
    ]
  },
  "pairlists": [
    {
      "method": "VolumePairList",
      "number_assets": 7,
      "sort_key": "quoteVolume",
      "min_value": 10000000,
      "refresh_period": 3600
    },
    {
      "method": "AgeFilter",
      "min_days_listed": 7
    },
    {
      "method": "PrecisionFilter"
    },
    {
      "method": "PriceFilter",
      "low_price_ratio": 0.01,
      "min_price": 0.0001
    },
    {
      "method": "SpreadFilter",
      "max_spread_ratio": 0.005
    }
  ],
  "unfilledtimeout": {
    "entry": 10,
    "exit": 30
  },
  "entry_pricing": {
    "price_side": "same",
    "use_order_book": false,
    "order_book_top": 1
  },
  "exit_pricing": {
    "price_side": "same",
    "use_order_book": false,
    "order_book_top": 1
  },
  "api_server": {
    "enabled": true,
    "listen_ip_address": "0.0.0.0",
    "listen_port": 8080,
    "username": "YOUR_USERNAME_HERE",
    "password": "YOUR_PASSWORD_HERE",
    "jwt_secret_key": "YOUR_JWT_SECRET_HERE",
    "ws_token": [
      "YOUR_WS_TOKEN_HERE"
    ],
    "CORS_origins": [
      "http://localhost:3000",
      "http://localhost:14251",
      "https://ui01.subx.fun",
      "https://ftui.subx.fun"
    ]
  },
  "strategy_path": "/freqtrade/user_data/strategies"
}
