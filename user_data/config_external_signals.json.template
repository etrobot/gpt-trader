{
  "bot_name": "external-signals-bot",
  "strategy": "classic_strategy.py",
  "dry_run": true,
  "timeframe": "5m",
  "startup_candle_count": 50,
  "max_open_trades": 3,
  "stake_currency": "USDT",
  "stake_amount": 100,
  "tradable_balance_ratio": 0.99,
  "fiat_display_currency": "USD",
  "force_entry_enable": true,
  "exchange": {
    "name": "bybit",
    "key": "YOUR_API_KEY_HERE",
    "secret": "YOUR_API_SECRET_HERE",
    "ccxt_config": {
      "enableRateLimit": true,
      "rateLimit": 50
    },
    "ccxt_async_config": {
      "enableRateLimit": true,
      "rateLimit": 50
    },
    "pair_whitelist": [
      "BTC/USDT",
      "ETH/USDT",
      "SOL/USDT",
      "BNB/USDT",
      "ADA/USDT"
    ],
    "pair_blacklist": []
  },
  "pairlists": [
    {
      "method": "StaticPairList"
    }
  ],
  "unfilledtimeout": {
    "entry": 10,
    "exit": 30
  },
  "entry_pricing": {
    "price_side": "same",
    "use_order_book": false,
    "order_book_top": 1
  },
  "exit_pricing": {
    "price_side": "same",
    "use_order_book": false,
    "order_book_top": 1
  },
  "api_server": {
    "enabled": true,
    "listen_ip_address": "0.0.0.0",
    "listen_port": 8080,
    "username": "YOUR_USERNAME_HERE",
    "password": "YOUR_PASSWORD_HERE",
    "jwt_secret_key": "YOUR_JWT_SECRET_HERE",
    "ws_token": [
      "YOUR_WS_TOKEN_HERE"
    ],
    "CORS_origins": [
      "http://localhost:14251"
    ]
  },
  "strategy_path": "/freqtrade/user_data/strategies"
}